{
  "name": "puppeteer-table-parser",
  "description": "Library to make parsing website tables much easier!",
  "keywords": [
    "puppeteer",
    "puppeteer table parser",
    "parsing html tables"
  ],
  "version": "1.1.1",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:Tomas2D/puppeteer-table-parser.git",
  "author": "Tomáš Dvořák <toomas2d@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -Rf lib; tsc -p tsconfig.build.json",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "changelog": "gitmoji-changelog",
    "release": "yarn version",
    "version": "yarn changelog && code --wait CHANGELOG.md && git add README.md CHANGELOG.md",
    "prepublishOnly": "yarn build",
    "test": "jest"
  },
  "peerDependencies": {
    "puppeteer": "^8.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "express": "^4.17.1",
    "gitmoji-changelog": "^2.2.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "puppeteer": "^8.0.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint \"{src,test}/**/*.ts\" --fix"
    ]
  }
}
